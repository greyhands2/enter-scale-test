header.header
  nav.nav.nav--tours
    a.nav__el(href='/') Staffs
  .header__logo
    img(src='/img/logo-white.png' alt='Enterscale logo')
  nav.nav.nav--user
    if user
      a.nav__el.nav__el--logout Log out
      a.nav__el(href='/me')
        img.nav__user-img(src=`/img/staffs/${staff.photo}` alt=`Photo of ${user.name}`)
        span= staff.firstName
    else
      a.nav__el(href='/login') Log in
      a.nav__el.nav__el--cta(href='#') Sign up
